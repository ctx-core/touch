{
	"name": "@ctx-core/touch",
	"version": "8.3.30",
	"description": "ctx-core touch",
	"keywords": [
		"ctx-core",
		"touch"
	],
	"homepage": "https://github.com/ctx-core/touch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/touch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/touch.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./no_touch__mount",
		"./ontouchmove__disable",
		"./ontouchmove__enable",
		"./touch_scrolling__disable",
		"./touch_scrolling__enable"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.1.30"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.5.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}